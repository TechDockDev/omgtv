{
    "name": "@pocketlol/content-service",
    "version": "0.1.0",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc --noEmit false --outDir dist",
        "start": "node dist/server.js",
        "generate": "prisma generate",
        "migrate:dev": "prisma migrate dev",
        "migrate:deploy": "prisma migrate deploy",
        "typecheck": "tsc --noEmit",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "test:contracts": "vitest run tests/mobile-contract.test.ts",
        "test:mobile-load": "autocannon -c 20 -d 15 http://localhost:4600/api/v1/content/mobile/home"
    },
    "engines": {
        "node": ">=18.18"
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@fastify/helmet": "^11.1.1",
        "@fastify/sensible": "^5.1.1",
        "@google-cloud/pubsub": "^4.3.3",
        "@google-cloud/storage": "^7.19.0",
        "@grpc/grpc-js": "^1.11.1",
        "@grpc/proto-loader": "^0.7.9",
        "@opentelemetry/api": "~1.7.0",
        "@opentelemetry/auto-instrumentations-node": "^0.46.0",
        "@opentelemetry/exporter-metrics-otlp-http": "^0.46.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
        "@opentelemetry/resources": "^1.19.0",
        "@opentelemetry/sdk-metrics": "^1.19.0",
        "@opentelemetry/sdk-node": "^0.46.0",
        "@opentelemetry/semantic-conventions": "^1.19.0",
        "@prisma/client": "^5.10.2",
        "dotenv": "^16.3.1",
        "fastify": "^4.25.2",
        "fastify-plugin": "^4.5.1",
        "fastify-type-provider-zod": "^2.0.0",
        "ioredis": "^5.4.1",
        "pino": "^9.0.0",
        "pino-pretty": "^11.0.0",
        "undici": "^6.13.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.10.7",
        "autocannon": "^7.14.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "prettier": "^3.1.1",
        "prisma": "^5.10.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3",
        "vitest": "^1.6.0"
    }
}
